<%- include('../partials/head', { title, description, ogImage }) %>
<%- include('../partials/header') %>

<!-- branchen.css lazy laden -->
<link rel="preload" href="/branchen.css?v=<%= encodeURIComponent(assetVersion || '1') %>" as="style" onload="this.rel='stylesheet'" />
<noscript>
